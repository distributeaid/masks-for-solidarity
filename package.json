{
  "name": "@distributeaid/masks-for-solidarity",
  "version": "0.0.0-development",
  "description": "Website for the Masks for Solidarity project",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "postinstall": "check-node-version --package",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build": "gatsby build --prefix-paths",
    "start": "gatsby develop"
  },
  "keywords": [
    "Distribute Aid",
    "Masks for Solidarity",
    "GatsbyJS",
    "React",
    "TypeScript",
    "Netlify",
    "GitHub Actions"
  ],
  "author": "Distribute Aid | https://distributeaid.org/",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/distributeaid/masks-for-solidarity/issues"
  },
  "homepage": "https://github.com/distributeaid/masks-for-solidarity#readme",
  "dependencies": {
    "feather-icons": "^4.28.0",
    "gatsby-remark-autolink-headers": "^2.3.7",
    "snyk": "^1.349.0"
  },
  "snyk": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@bifravst/code-style": "^8.0.4",
    "@bifravst/eslint-config-typescript": "^5.0.1",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-angular": "^9.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "check-node-version": "^4.0.3",
    "eslint-plugin-react": "^7.20.2",
    "gatsby": "^2.23.12",
    "gatsby-cli": "^2.12.52",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-styled-components": "^3.3.6",
    "gatsby-plugin-typescript": "^2.4.8",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-source-sanity": "^6.0.1",
    "gatsby-transformer-remark": "^2.8.20",
    "hast-to-hyperscript": "^9.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "sanitize-filename": "^1.6.3",
    "styled-components": "^5.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
